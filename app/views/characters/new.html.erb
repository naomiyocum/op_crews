<%= form_with url: "/crews/#{@crew.id}/characters", method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %><br><br>

  <%= form.label :age %>
  <%= form.text_field :age %><br><br>

  <%= form.label :devil_fruit_eater, id:"devil" %>
  <%= form.text_field :devil_fruit_eater, id:"devil" %><br><br>
  
  <%= form.label :epithet %>
  <%= form.text_field :epithet %><br><br>

  <%= form.label :bounty %>
  <%= form.text_field :bounty %><br><br>

  <%= form.label :crew_id, id:"crewid" %>
  <%= form.text_field :crew_id, id:"crewid" %><br><br>

  <%= form.submit "Create Member" %>
<% end %>