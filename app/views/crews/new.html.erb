<%= form_with url: '/crews', method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %><br><br>

  <%= form.label :ranking %>
  <%= form.text_field :ranking %><br><br>

  <%= form.label :ship %>
  <%= form.text_field :ship %><br><br>

  <%= form.label :worst_generation, id:"worst" %>
  <%= form.select :worst_generation, [true, false], id:"worst" %><br><br>

  <%= form.submit "Create Crew" %>

<% end %>